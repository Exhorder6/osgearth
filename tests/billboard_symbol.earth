<map name="Billboard Demo" type="geocentric" version="2">
	<model name="TFSBillboards" driver="feature_geom">
        <features name="buildings" driver="tfs">
            <url>../data/tfs_boston.zip/layer/tfs.xml</url>
            <format>json</format>
        </features>
	    <styles>
            <style type="text/css">
                ForestStyle1 {
                   billboard2-image:../data/splat/pine.png;
                   billboard2-width:8;
				   billboard2-height:14;
				   billboard2-density: 1000;
				   billboard2-scaleVariance: 0.4;
				   billboard2-brightnessVariance: 0.7;
				   billboard2-randomSeed: 0;
				   billboard2-alphaRefValue: 0.1;
				   billboard2-alphaBlend: 0;
				   altitude-clamping: terrain;
                }
				ForestStyle2 {
                   billboard2-image:../data/tree.gif;
                   billboard2-width:6;
				   billboard2-height:8;
				   billboard2-density: 1000;
				   billboard2-scaleVariance: 0.4;
				   billboard2-brightnessVariance: 0.1;
				   billboard2-randomSeed: 2;
				   billboard2-alphaRefValue: 0.4;
				   billboard2-alphaBlend: 1;
				   altitude-clamping: terrain;
                }
		    </style>
			<!--<selector class="ForestStyle1"/>
			<selector class="ForestStyle2"/>-->
        </styles>   
    </model>	
    <model name="ShapeBillboards" driver="feature_geom">
        <features name="parks" driver="ogr">
           <url>../data/parks.shp</url>
            <build_spatial_index>true</build_spatial_index>
        </features>
		
		<!--<features name="ForestStyle2" driver="ogr">
            <url>../data/points.shp</url>
	        <build_spatial_index>true</build_spatial_index>
        </features>-->
		
